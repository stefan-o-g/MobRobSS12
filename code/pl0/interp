func main(): None = 
	consts a = 3+5;
	
	func debug():None = native
	func print_int(i:Int):None = native
	func print_double(d:Double):None = native

	func min(a:Int, b:Int):Int = if a < b then a else b
	func max(a:Int, b:Int):Int = if a > b then a else b
	func fac(n:Int):Int = if n == 0 then 1 else n*fac(n-1)


{
	print_int(fac(10))
